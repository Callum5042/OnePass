<Window x:Class="OnePass.WPF.Windows.SyncWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OnePass.WPF.Windows"
        mc:Ignorable="d"
        SourceInitialized="Window_SourceInitialized" Closed="Window_Closed"
        Title="Sync" Height="auto" Width="300" ResizeMode="NoResize" SizeToContent="Height" WindowStartupLocation="CenterScreen">
    <StackPanel>
        <TextBox x:Name="Textbox" AcceptsReturn="True" IsReadOnly="True"></TextBox>
    </StackPanel>
</Window>
