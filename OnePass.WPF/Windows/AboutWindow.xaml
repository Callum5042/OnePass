<Window x:Class="OnePass.Windows.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OnePass.Windows"
        mc:Ignorable="d"
        Title="About" Height="200" Width="200" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <StackPanel HorizontalAlignment="Center">
        <TextBlock x:Name="versionTextBlock">Version: </TextBlock>
    </StackPanel>
</Window>
